[user]
  name = 
  email = 
[color]
  diff = auto
  sh = auto
  status = auto
  branch = auto
[github]
  user = manboubird
[include]
  # git ver >= 1.7.10 
  path = .gitconfig.local
 [alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dc = diff --cached
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files

